{
  "name": "zengest-backend",
  "version": "1.0.0",
  "description": "Backend API pour l'application de gestion de restaurant Zengest",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:legacy": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "node src/scripts/seedData.js",
    "seed:complete": "node src/scripts/seedDataComplete.js",
    "seed:dev": "NODE_ENV=development node src/scripts/seedData.js",
    "seed:menu": "node src/scripts/seedMenu.js",
    "test:floorplans": "bash src/scripts/test_floorplans.sh",
    "test:menu": "bash src/scripts/test_menu.sh",
    "test:all": "bash src/scripts/test_floorplans.sh && echo '' && bash src/scripts/test_menu.sh"
  },
  "keywords": [
    "restaurant",
    "api",
    "nodejs",
    "express",
    "mongodb",
    "jwt",
    "gestion"
  ],
  "author": "Zengest Team",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.0",
    "zengest-backend": "file:"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1"
  }
}
